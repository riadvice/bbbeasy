    "web-vitals": "^3.0.1",
    "webpack": "^5.74.0",
    "websocket": "^1.0.34"
  },
  "scripts": {
    "start-dev": "hivelvet -ew && PORT=3300 NODE_ENV=development INSTALLER_FEATURE=false craco start",
    "start-dev-installer": "hivelvet -ei && PORT=3300 NODE_ENV=development INSTALLER_FEATURE=true craco start",
    "build": "INSTALLER_FEATURE=false craco build",
    "build-installer": "INSTALLER_FEATURE=true craco build",
    "nyc": "nyc instrument --compact=false src instrumented",
    "nyc-summary": "nyc report --reporter=text-summary",
    "cypress": "cypress run && nyc report --reporter=html && npm run nyc-summary",
    "analyze": "craco analyze",
    "test": "craco test",
    "eject": "craco eject",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src --color",
    "format": "prettier --write 'src/**/*.{ts,tsx,scss,css,json}'",
    "isready": "npm run format && npm run lint && npm run build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not ie <= 11"
    ],
    "development": [
      "defaults",
      "not IE 11"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.18.10",
    "@craco/craco": "^6.4.3",
    "@crowdin/cli": "3",
    "@cypress/code-coverage": "^3.9.12",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^14.4.2",
    "@types/node": "^17.0.23",
    "@types/pino": "^7.0.5",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.18.0",
    "babel-plugin-import": "^1.13.5",
    "babel-plugin-istanbul": "^6.1.1",
    "cypress": "^10.8.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^27.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.21.1",
    "eslint-webpack-plugin": "^3.2.0",
    "husky": "8.0.1",
    "lint-staged": "^13.0.3",
    "nyc": "^15.1.0",
    "prettier": "^2.7.1",
    "react-styleguidist": "^11.2.0",
    "simple-progress-webpack-plugin": "^2.0.0",
    "source-map-explorer": "^2.5.2",
    "ts-loader": "^9.3.1"
  },
  "packageManager": "yarn@3.2.3"
}
