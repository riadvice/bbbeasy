    "istanbul-lib-coverage": "^3.2.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "perfect-scrollbar": "^1.5.5",
    "pg": "^8.8.0",
    "pino": "^8.4.2",
    "pino-pretty": "^9.1.0",
    "pino-transmit-http": "^1.0.1",
    "pm2": "^5.2.0",
    "rc-color-picker": "^1.2.6",
    "react": "^17.0.2",
    "react-color": "^2.19.3",
    "react-dom": "^17.0.2",
    "react-highlight-words": "^0.18.0",
    "react-hook-form": "^7.34.0",
    "react-i18next": "^11.18.3",
    "react-intl": "^6.1.1",
    "react-router-dom": "^6.3.0",
